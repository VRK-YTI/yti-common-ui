{
  "name": "yti-common-ui",
  "version": "1.0.0",
  "description": "Shared YTI UI modules",
  "main": "index.ts",
  "license": "EUPL-1.1",
  "scripts": {
    "create-translations": "node -- node_modules/@biesbjerg/ngx-translate-extract/bin/cli.js --input ./components --output ./po/template.pot --format=pot --clean",
    "ng": "ng",
    "start": "ng serve",
    "build:prod": "ng build yti-common-ui --configuration production",
    "build": "ng build yti-common-ui --configuration development",
    "watch": "ng build yti-common-ui --watch --configuration development",
    "pack": "cd dist/yti-common-ui && npm pack",
    "publish": "node publish.js",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "bootstrap": "^4.5.0",
    "@ngx-translate/core": "^13.0.0",
    "ngx-clipboard": "^14.0.1",
    "material-design-icons": "^3.0.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.5",
    "@angular/cli": "~12.0.1",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@angular/compiler-cli": "~12.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "typescript": "~4.2.3",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9"
  }
}
